<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="@winkjs_org" />
<meta name="twitter:creator" content="@<%= current_article.data.author_twitter %>" />
<meta property="og:url" content="https://winkjs.org/<%= current_page.path %>" />
<meta property="og:title" content="<%= current_article.title %>" />
<meta property="og:description" content="<%= current_article.data.excerpt %>" />
<meta property="og:image" content="https://winkjs.org/images/<%= current_article.data.image %>" />
